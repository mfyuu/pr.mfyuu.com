{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "pr-namidapoo",
	"compatibility_date": "2025-06-19",
	"compatibility_flags": [
		"nodejs_compat"
	],
	"main": ".cloudflare/worker.js",
	"assets": {
		"directory": ".cloudflare/public",
		"binding": "ASSETS",
		"html_handling": "drop-trailing-slash",
		"run_worker_first": false
	}
}
